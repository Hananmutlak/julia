{
  "name": "julia",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "npm run transpile && npm run minify-css && npm run minify-js && npm run optimize-images && parcel build src/index.html",
    "transpile": "babel src/js --out-dir dist/js",
    "minify-css": "npx cssnano src/css/*.css --output dist/css/",
    "minify-js": "terser src/js/*.js --compress --mangle --output dist/js/",
    "optimize-images": "imagemin src/images/* --out-dir=dist/images && imagemin-webp dist/images/* -o dist/images/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "cssnano": "^7.0.6",
    "imagemin": "^7.0.0",
    "imagemin-webp": "^6.0.0",
    "parcel": "^2.13.3",
    "postcss-cli": "^11.0.0",
    "terser": "^5.7.2"
  },
"browserslist": [
  ">0.2%",
  "not dead",
  "not op_mini all"
]

}
